{
  "name": "architect-js",
  "version": "0.1.0",
  "description": "Client library for the Architect trading platform.",
  "main": "src/index.mjs",
  "engines": {
    "node": ">=20.11.0",
    "npm": ">=10.8.2"
  },
  "scripts": {
    "codegen": "graphql-codegen",
    "codegen:watch": "graphql-codegen --watch \"./codegen/**/*\"",
    "format": "biome format --write",
    "typecheck": "tsc --noEmit",
    "test": "node --env-file=./.env.test --test",
    "test:credentials": "node --env-file=../../.env --test",
    "prerelease": "node ./scripts/prerelease.mjs"
  },
  "author": "Architect Financial Technologies, Inc. <hello@architect.xyz>",
  "license": "Apache-2.0",
  "type": "module",
  "dependencies": {
    "gql.tada": "^1.8.10",
    "graphql-http": "^1.22.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@graphql-codegen/plugin-helpers": "^5.0.4",
    "@graphql-codegen/cli": "^5.0.3",
    "@parcel/watcher": "^2.4.1",
    "@tsconfig/node-lts": "^20.1.3",
    "@types/node": "^22.7.4",
    "typescript": "^5.6.2",
    "zx": "^8.1.9"
  }
}
