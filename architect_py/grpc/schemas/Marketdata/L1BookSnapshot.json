{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "L1BookSnapshot",
  "type": "object",
  "required": [
    "s",
    "tn",
    "ts"
  ],
  "properties": {
    "a": {
      "title": "best_ask",
      "description": "(price, quantity)",
      "type": [
        "array",
        "null"
      ],
      "items": [
        {
          "type": "number",
          "format": "decimal"
        },
        {
          "type": "number",
          "format": "decimal"
        }
      ]
    },
    "b": {
      "title": "best_bid",
      "description": "(price, quantity)",
      "type": [
        "array",
        "null"
      ],
      "items": [
        {
          "type": "number",
          "format": "decimal"
        },
        {
          "type": "number",
          "format": "decimal"
        }
      ]
    },
    "rt": {
      "title": "recv_time",
      "description": "Time that Architect feed received the message; only set if streaming from direct L1 feeds",
      "type": [
        "integer",
        "null"
      ],
      "format": "int64"
    },
    "rtn": {
      "title": "recv_time_ns",
      "type": [
        "integer",
        "null"
      ],
      "format": "default"
    },
    "s": {
      "title": "symbol",
      "type": "string"
    },
    "tn": {
      "title": "timestamp_ns",
      "type": "integer",
      "format": "default",
      "minimum": 0.0
    },
    "ts": {
      "title": "timestamp",
      "type": "integer",
      "format": "int64"
    }
  }
}