{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "LicenseInfoResponse",
  "type": "object",
  "required": [
    "not_after",
    "not_before",
    "pubkey",
    "subject"
  ],
  "properties": {
    "not_after": {
      "type": "string",
      "format": "date-time"
    },
    "not_before": {
      "type": "string",
      "format": "date-time"
    },
    "pubkey": {
      "description": "PEM-encoded public key",
      "type": "string"
    },
    "subject": {
      "type": "string"
    }
  }
}