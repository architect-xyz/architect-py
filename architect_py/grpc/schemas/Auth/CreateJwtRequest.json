{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "CreateJwtRequest",
  "description": "Create a session JWT to use for authentication with upstream gRPC services.",
  "type": "object",
  "required": [
    "api_key",
    "api_secret"
  ],
  "properties": {
    "api_key": {
      "type": "string"
    },
    "api_secret": {
      "type": "string"
    }
  },
  "route": "/architect.Auth/CreateJwt",
  "rpc_method": "unary",
  "service": "Auth",
  "response_type": "CreateJwtResponse"
}