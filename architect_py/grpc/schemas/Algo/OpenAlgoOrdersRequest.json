{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "OpenAlgoOrdersRequest",
  "description": "Find all open algo orders matching the given criteria.\n\nIf limit is not specified, it will default to 100.",
  "type": "object",
  "properties": {
    "account": {
      "default": null,
      "anyOf": [
        {
          "$ref": "../definitions.json#/AccountIdOrName"
        },
        {
          "type": "null"
        }
      ]
    },
    "algo": {
      "type": [
        "string",
        "null"
      ]
    },
    "display_symbol": {
      "type": [
        "string",
        "null"
      ]
    },
    "from_inclusive": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time"
    },
    "limit": {
      "type": [
        "integer",
        "null"
      ],
      "format": "default"
    },
    "order_ids": {
      "description": "if order_ids is not empty, the limit field is ignored",
      "type": [
        "array",
        "null"
      ],
      "items": {
        "$ref": "../definitions.json#/OrderId"
      }
    },
    "parent_order_id": {
      "anyOf": [
        {
          "$ref": "../definitions.json#/OrderId"
        },
        {
          "type": "null"
        }
      ]
    },
    "status": {
      "type": [
        "array",
        "null"
      ],
      "items": {
        "$ref": "../definitions.json#/AlgoOrderStatus"
      }
    },
    "to_exclusive": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time"
    },
    "trader": {
      "anyOf": [
        {
          "$ref": "../definitions.json#/TraderIdOrEmail"
        },
        {
          "type": "null"
        }
      ]
    }
  },
  "route": "/json.architect.Algo/OpenAlgoOrders",
  "rpc_method": "unary",
  "service": "Algo",
  "response_type": "AlgoOrdersResponse"
}