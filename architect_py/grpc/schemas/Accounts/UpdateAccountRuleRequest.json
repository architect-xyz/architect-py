      "type": [
        "boolean",
        "null"
      ]
    },
    "max_net_pos_qty": {
      "type": [
        "string",
        "null"
      ],
      "format": "decimal"
    },
    "max_order_qty": {
      "type": [
        "string",
        "null"
      ],
      "format": "decimal"
    },
    "min_net_pos_qty": {
      "type": [
        "string",
        "null"
      ],
      "format": "decimal"
    },
    "specificity": {
      "type": "integer",
      "format": "int32"
    },
    "target": {
      "type": "string"
    },
    "trading_enabled": {
      "type": [
        "boolean",
        "null"
      ]
    }
  },
  "route": "/json.architect.Accounts/UpdateAccountRule",
