{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "OpenPaperAccountResponse",
  "type": "object",
  "required": [
    "success"
  ],
  "properties": {
    "account_id": {
      "description": "The ID of the newly created paper account (None if failed)",
      "type": [
        "string",
        "null"
      ],
      "format": "uuid"
    },
    "error": {
      "description": "Error message if the operation failed",
      "type": [
        "string",
        "null"
      ]
    },
    "success": {
      "type": "boolean"
    }
  }
}