{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "OpenPaperAccountRequest",
  "type": "object",
  "required": [
    "account_name"
  ],
  "properties": {
    "account_name": {
      "description": "The name for the new paper account (will be prefixed with PAPER:{email}:)",
      "type": "string"
    }
  },
  "route": "/json.architect.Accounts/OpenPaperAccount",
  "rpc_method": "unary",
  "service": "Accounts",
  "response_type": "OpenPaperAccountResponse"
}