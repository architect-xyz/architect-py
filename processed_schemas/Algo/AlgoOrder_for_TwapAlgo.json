{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "AlgoOrder_for_TwapAlgo",
  "type": "object",
  "required": [
    "account",
    "algo_name",
    "algo_order_id",
    "create_time",
    "params",
    "state",
    "status",
    "trader"
  ],
  "properties": {
    "account": {
      "type": "string",
      "format": "uuid"
    },
    "algo_name": {
      "type": "string"
    },
    "algo_order_id": {
      "$ref": "../definitions.json#/OrderId"
    },
    "create_time": {
      "type": "string",
      "format": "date-time"
    },
    "display_symbols": {
      "type": [
        "array",
        "null"
      ],
      "items": {
        "type": "string"
      }
    },
    "last_error": {
      "type": [
        "string",
        "null"
      ]
    },
    "last_error_time": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time"
    },
    "params": {
      "$ref": "../definitions.json#/TwapParams"
    },
    "parent_order_id": {
      "anyOf": [
        {
          "$ref": "../definitions.json#/OrderId"
        },
        {
          "type": "null"
        }
      ]
    },
    "state": {
      "$ref": "../definitions.json#/AlgoState"
    },
    "status": {
      "$ref": "../definitions.json#/TwapStatus"
    },
    "trader": {
      "$ref": "../definitions.json#/UserId"
    }
  }
}