{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Order",
  "type": "object",
  "oneOf": [
    {
      "type": "object",
      "required": [
        "k",
        "p",
        "po"
      ],
      "properties": {
        "k": {
          "type": "string",
          "enum": [
            "LIMIT"
          ]
        },
        "p": {
          "title": "limit_price",
          "allOf": [
            {
              "type": "string",
              "format": "decimal"
            }
          ]
        },
        "po": {
          "title": "post_only",
          "type": "boolean"
        }
      }
    },
    {
      "type": "object",
      "required": [
        "k",
        "p",
        "tp"
      ],
      "properties": {
        "k": {
          "type": "string",
          "enum": [
            "STOP_LOSS_LIMIT"
          ]
        },
        "p": {
          "title": "limit_price",
          "allOf": [
            {
              "type": "string",
              "format": "decimal"
            }
          ]
        },
        "tp": {
          "title": "trigger_price",
          "allOf": [
            {
              "type": "string",
              "format": "decimal"
            }
          ]
        }
      }
    },
    {
      "type": "object",
      "required": [
        "k",
        "p",
        "tp"
      ],
      "properties": {
        "k": {
          "type": "string",
          "enum": [
            "TAKE_PROFIT_LIMIT"
          ]
        },
        "p": {
          "title": "limit_price",
          "allOf": [
            {
              "type": "string",
              "format": "decimal"
            }
          ]
        },
        "tp": {
          "title": "trigger_price",
          "allOf": [
            {
              "type": "string",
              "format": "decimal"
            }
          ]
        }
      }
    }
  ],
  "required": [
    "a",
    "d",
    "id",
    "o",
    "q",
    "s",
    "src",
    "tif",
    "tn",
    "ts",
    "u",
    "ve",
    "xq"
  ],
  "properties": {
    "a": {
      "title": "account",
      "type": "string",
      "format": "uuid"
    },
    "d": {
      "title": "dir",
      "allOf": [
        {
          "$ref": "../definitions.json#/Dir"
        }
      ]
    },
    "id": {
      "$ref": "../definitions.json#/OrderId"
    },
    "o": {
      "title": "status",
      "allOf": [
        {
          "$ref": "../definitions.json#/OrderStatus"
        }
      ]
    },
    "pid": {
      "title": "parent_id",
      "anyOf": [
        {
          "$ref": "../definitions.json#/OrderId"
        },
        {
          "type": "null"
        }
      ]
    },
    "q": {
      "title": "quantity",
      "allOf": [
        {
          "type": "string",
          "format": "decimal"
        }
      ]
    },
    "r": {
      "title": "reject_reason",
      "anyOf": [
        {
          "$ref": "../definitions.json#/OrderRejectReason"
        },
        {
          "type": "null"
        }
      ]
    },
    "rm": {
      "title": "reject_message",
      "type": [
        "string",
        "null"
      ]
    },
    "s": {
      "title": "symbol",
      "type": "string"
    },
    "src": {
      "title": "source",
      "allOf": [
        {
          "$ref": "../definitions.json#/OrderSource"
        }
      ]
    },
    "tif": {
      "title": "time_in_force",
      "allOf": [
        {
          "$ref": "../definitions.json#/TimeInForce"
        }
      ]
    },
    "tn": {
      "title": "recv_time_ns",
      "type": "integer",
      "format": "default",
      "minimum": 0.0
    },
    "ts": {
      "title": "recv_time",
      "type": "integer",
      "format": "int64"
    },
    "u": {
      "title": "trader",
      "allOf": [
        {
          "$ref": "../definitions.json#/UserId"
        }
      ]
    },
    "ve": {
      "title": "execution_venue",
      "type": "string"
    },
    "xp": {
      "title": "average_fill_price",
      "anyOf": [
        {
          "type": "string",
          "format": "decimal"
        },
        {
          "type": "null"
        }
      ]
    },
    "xq": {
      "title": "filled_quantity",
      "allOf": [
        {
          "type": "string",
          "format": "decimal"
        }
      ]
    }
  }
}