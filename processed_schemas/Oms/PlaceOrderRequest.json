{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "PlaceOrderRequest",
  "type": "object",
  "required": [
    "d",
    "q",
    "s",
    "tif",
    "k",
    "p"
  ],
  "properties": {
    "a": {
      "title": "account",
      "default": null,
      "anyOf": [
        {
          "$ref": "../definitions.json#/AccountIdOrName"
        },
        {
          "type": "null"
        }
      ]
    },
    "d": {
      "title": "dir",
      "allOf": [
        {
          "$ref": "../definitions.json#/Dir"
        }
      ]
    },
    "id": {
      "description": "If not specified, one will be generated for you; note, in that case, you won't know for sure if the specific request went through.",
      "anyOf": [
        {
          "$ref": "../definitions.json#/OrderId"
        },
        {
          "type": "null"
        }
      ]
    },
    "pid": {
      "title": "parent_id",
      "anyOf": [
        {
          "$ref": "../definitions.json#/OrderId"
        },
        {
          "type": "null"
        }
      ]
    },
    "q": {
      "title": "quantity",
      "type": "number",
      "format": "decimal"
    },
    "s": {
      "title": "symbol",
      "type": "string"
    },
    "src": {
      "title": "source",
      "default": null,
      "anyOf": [
        {
          "$ref": "../definitions.json#/OrderSource"
        },
        {
          "type": "null"
        }
      ]
    },
    "tif": {
      "title": "time_in_force",
      "allOf": [
        {
          "$ref": "../definitions.json#/TimeInForce"
        }
      ]
    },
    "u": {
      "title": "trader",
      "default": null,
      "anyOf": [
        {
          "$ref": "../definitions.json#/TraderIdOrEmail"
        },
        {
          "type": "null"
        }
      ]
    },
    "x": {
      "title": "execution_venue",
      "default": null,
      "type": [
        "string",
        "null"
      ]
    },
    "p": {
      "title": "limit_price",
      "type": "number",
      "format": "decimal"
    },
    "po": {
      "title": "post_only",
      "type": "boolean"
    },
    "tp": {
      "title": "trigger_price",
      "type": "number",
      "format": "decimal"
    },
    "k": {
      "type": "string",
      "title": "place_order_request_type",
      "enum": [
        "LIMIT",
        "STOP_LOSS_LIMIT",
        "TAKE_PROFIT_LIMIT"
      ]
    }
  },
  "route": "/json.architect.Oms/PlaceOrder",
  "unary_type": "unary",
  "service": "Oms",
  "response_type": "Order",
  "enum_tag_union": [
    "po",
    "p",
    "k",
    "tp"
  ],
  "enum_tag": "k",
  "enum_tag_to_other_required_keys": {
    "LIMIT": [
      "k",
      "p",
      "po"
    ],
    "STOP_LOSS_LIMIT": [
      "k",
      "p",
      "tp"
    ],
    "TAKE_PROFIT_LIMIT": [
      "k",
      "p",
      "tp"
    ]
  }
}